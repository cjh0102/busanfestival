<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/LinearLayout1"	android:layout_width="match_parent"	android:layout_height="match_parent"	android:background="@drawable/main_background"	android:orientation="horizontal"	android:weightSum="10">	<View		android:id="@+id/view2"		android:layout_width="0px"		android:layout_height="wrap_content"		android:layout_weight="0.5"/>	<LinearLayout		android:layout_width="0px"		android:layout_height="match_parent"		android:layout_weight="9"		android:orientation="vertical"		android:weightSum="1">		<View			android:id="@+id/view5"			android:layout_width="fill_parent"			android:layout_height="0px"			android:layout_weight="0.05"/>		<LinearLayout			android:layout_width="fill_parent"			android:layout_height="0px"			android:layout_weight="0.13"			android:weightSum="6">			<ImageView				android:id="@+id/quizImageView"				android:layout_width="0px"				android:layout_height="match_parent"				android:layout_weight="1.5"				android:background="@drawable/main_quiz"/>			<View				android:id="@+id/view1"				android:layout_width="0px"				android:layout_height="wrap_content"				android:layout_weight="3.0"/>			<ImageView				android:id="@+id/settingImageView"				android:layout_width="0px"				android:layout_height="match_parent"				android:layout_weight="1.5"				android:background="@drawable/main_etc"/>		</LinearLayout>		<LinearLayout			android:layout_width="match_parent"			android:layout_height="0px"			android:layout_weight="0.37"			android:weightSum="5">			<ImageView				android:id="@+id/sunImageView"				android:layout_width="0px"				android:layout_height="match_parent"				android:layout_weight="1.5"				android:background="@drawable/main_sun"/>			<LinearLayout				android:layout_width="0px"				android:layout_height="match_parent"				android:layout_weight="3.5"				android:orientation="vertical"				android:weightSum="4">				<ImageView					android:id="@+id/rockImageView"					android:layout_width="match_parent"					android:layout_height="0px"					android:layout_weight="1.5"					android:background="@drawable/main_rock"/>				<LinearLayout					android:layout_width="match_parent"					android:layout_height="0px"					android:layout_weight="2.5"					android:weightSum="4">					<ImageView						android:id="@+id/portImageView"						android:layout_width="0px"						android:layout_height="match_parent"						android:layout_weight="2.52"						android:background="@drawable/main_port"/>					<ImageView						android:id="@+id/flameImageView"						android:layout_width="0px"						android:layout_height="fill_parent"						android:layout_weight="1.48"						android:background="@drawable/main_flame1"/>				</LinearLayout>			</LinearLayout>		</LinearLayout>		<LinearLayout			android:layout_width="match_parent"			android:layout_height="0px"			android:layout_weight="0.14"			android:weightSum="1">			<ImageView				android:id="@+id/seaImageView"				android:layout_width="0px"				android:layout_height="match_parent"				android:layout_weight="0.74"				android:background="@drawable/main_sea"/>			<ImageView				android:id="@+id/flameImageView2"				android:layout_width="0px"				android:layout_height="fill_parent"				android:layout_weight="0.26"				android:background="@drawable/main_flame2"/>		</LinearLayout>		<LinearLayout			android:layout_width="match_parent"			android:layout_height="0px"			android:layout_weight="0.3"			android:weightSum="10">			<View				android:id="@+id/view4"				android:layout_width="0px"				android:layout_height="wrap_content"				android:layout_weight="5.5"/>			<ImageView				android:id="@+id/characterImageView"				android:layout_width="0px"				android:layout_height="match_parent"				android:layout_weight="4.50"				android:background="@drawable/main_character"/>		</LinearLayout>	</LinearLayout>	<View		android:id="@+id/view3"		android:layout_width="0px"		android:layout_height="wrap_content"		android:layout_weight="0.5"/></LinearLayout>